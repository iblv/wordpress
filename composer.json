{
    "name": "gcode/wordpress-base",
    "description": "WordPress base site with some plugins",
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "aws/aws-sdk-php":"*",
        "wpackagist-plugin/akismet": "3.1.11",
        "wpackagist-plugin/amazon-web-services": "0.3.6",
        "wpackagist-plugin/amazon-s3-and-cloudfront": "1.0.4",
        "wpackagist-plugin/captcha": "4.2.0",
        "wpackagist-plugin/facebook": "1.5.5",
        "wpackagist-plugin/facebook-publish": "1.4.3",
        "wpackagist-plugin/wp-google-analytics": "1.4.1",
        "wpackagist-plugin/wp-mail-smtp": "0.9.6",
        "fancyguy/wordpress-monolog": "dev-master",
        "wpackagist-plugin/church-theme-content": "1.7",
        "wpackagist-theme/restful": "1.1.6",
        "wpackagist-plugin/brightslider": "1.1.0",
        "wpackagist-plugin/post-type-archive-links": "1.3.1",
        "wpackagist-plugin/rest-api": "2.0-beta13"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }
    }
}
